{
    "name": "solar-art-gallery",
    "version": "0.0.1",
    "description": "Solar Art Gallery Serverless Backend",
    "main": "serverless.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dynamoDB-local": "serverless dynamodb start",
        "dynamoDB-seed": "serverless dynamodb:seed",
        "sls-offline": "serverless offline",
        "offline": "run-p -l dynamoDB-local sls-offline",
        "sls-deploy": "serverless deploy --verbose",
        "deploy": "run-s -l sls-deploy dynamoDB-seed",
        "remove": "serverless remove --verbose"
    },
    "engines": {
        "node": ">=14.15.0"
    },
    "dependencies": {
        "@middy/core": "^1.5.2",
        "@middy/http-cors": "^2.2.0",
        "@middy/http-json-body-parser": "^1.5.2",
        "aws-xray-sdk": "^3.3.3",
        "axios": "^0.21.1",
        "dotenv": "^9.0.2",
        "http-errors": "^1.8.0",
        "jsonwebtoken": "^8.5.1",
        "jwks-rsa": "^2.0.3",
        "source-map-support": "^0.5.19",
        "uuid": "^8.3.2",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@serverless/typescript": "^2.23.0",
        "@types/aws-lambda": "^8.10.71",
        "@types/http-errors": "^1.8.0",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/node": "^14.14.25",
        "aws-sdk": "^2.906.0",
        "json-schema-to-ts": "^1.5.0",
        "npm-run-all": "^4.1.5",
        "serverless": "^2.46.0",
        "serverless-dynamodb-local": "^0.2.39",
        "serverless-dynamodb-seed": "^0.3.0",
        "serverless-iam-roles-per-function": "^3.1.0",
        "serverless-offline": "^7.0.0",
        "serverless-pseudo-parameters": "^2.5.0",
        "serverless-s3-local": "^0.6.15",
        "serverless-s3-remover": "^0.6.0",
        "serverless-s3-sync": "^1.17.1",
        "serverless-webpack": "^5.3.5",
        "ts-loader": "^8.0.15",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typescript": "^4.1.3",
        "webpack": "^5.20.2",
        "webpack-node-externals": "^2.5.2"
    },
    "author": "Daniel Tortosa Marangoni (https://github.com/dtmarangoni/solar-art-gallery)",
    "license": "MIT"
}
